package exemplos;
//3) Faça uma tabela de multiplicação para o número 2 multiplicando do 1 até 10.
public class Exercicio3 {

	public static void main(String[] args) {
		int multiplicacao = 0;
		for (int i = 1; i <= 10; i++) {
			multiplicacao = i*2;
			System.out.println("2 x " + i + " = " + multiplicacao);
		}
	}

}
